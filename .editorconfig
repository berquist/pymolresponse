# The full list of properties is located at
# https://github.com/editorconfig/editorconfig/wiki/EditorConfig-Properties.

root = true

[*]
charset = utf-8
insert_final_newline = true
trim_trailing_whitespace = true

[*.py]
indent_style = space
indent_size = 4

[.travis.yml]
indent_style = space
indent_size = 2
