# The full list of properties is located at
# https://github.com/editorconfig/editorconfig/wiki/EditorConfig-Properties.

root = true

[*.py]
charset = utf-8
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

[.travis.yml]
indent_style = space
indent_size = 2
